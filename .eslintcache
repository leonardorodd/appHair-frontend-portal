                       value: 0,\n                                            }} */\n                                            onChange={value => {\n                                                console.log(value);\n                                                /*   setCurrentUf(\n                                                    value?.label.substring(\n                                                        0,\n                                                        2,\n                                                    ),\n                                                ); */\n                                                /*                                                 setCurrentCity(null);\n                                                 */\n                                            }}\n                                            options={ufsList}\n                                            isSearchable\n                                            blurInputOnSelect\n                                            openMenuOnFocus\n                                        />\n                                        <Select\n                                            name=\"cidade\"\n                                            label=\"Cidade\"\n                                            isSearchable\n                                            defaultValue={{\n                                                label: 'Selecione uma cidade',\n                                                value: 0,\n                                            }}\n                                            /* disabled={cities?.length === 0} */\n                                            noOptionsMessage={() =>\n                                                /* {\n                                                console.log(\n                                                    document.querySelector(\n                                                        'div.react-se